    "react-dom": "^19.1.1",
    "react-i18next": "^16.0.0",
    "react-router-dom": "^6.14.1"
  },
  "devDependencies": {
    "@chromatic-com/storybook": "^4.1.1",
    "@eslint/js": "^9.33.0",
    "@storybook/addon-a11y": "^9.1.10",
