    "husky": "^9.1.7",
    "jsdom": "^25.0.1",
    "lint-staged": "^16.1.6",
    "prettier": "^3.6.2",
    "storybook": "^9.1.10",
    "typescript": "~5.8.3",
    "typescript-eslint": "^8.43.0",
    "vite": "^7.1.4",
